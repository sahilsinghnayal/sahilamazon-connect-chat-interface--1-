    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "build": {
      "name": "build",
      "command": "npm run build"
    },
    "release": {
      "name": "release",
      "command": "npm run release"
    },
    "debug": {
      "name": "debug",
      "command": "npm run debug"
    },
    "dev-build": {
      "name": "dev-build",
      "command": "npm run dev-build"
    },
    "dev-watch": {
      "name": "dev-watch",
